using System;

public class Testprog
{
    public static void Main(string[] args)
    {
        
    for (int i=0; i < 4; i++)
    {
        for (int j=0; j < 4; j++)
        {
            if (i % 2 == 0)
            
                Console.Write(i+1);
            
            else
            
                Console.Write("E");
            
        }
        Console.WriteLine();
    }
    }
}
